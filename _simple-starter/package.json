{
  "name": "starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "serve -s -open .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "predeploy": "npm i && npm-run-all predeploy:*",
    "predeploy:add-base-href": "bash ./bin/gh-pages-base-href.bash index.html css-examples add",
    "predeploy:gh-pages-404": "ncp index.html 404.html",
    "predeploy:nojekyll": "touch .nojekyll",
    "deploy": "gh-pages -e ./ -d ./ -s '{*.*,.*,*,**}' --branch gh-pages --dotfiles=true -m \"Github Pages $(date +%Y-%m-%d) deployment\"",
    "postdeploy": "npm-run-all -p postdeploy:*",
    "postdeploy:remove-base-href": "bash ./bin/gh-pages-base-href.bash index.html css-examples remove",
    "postdeploy:remove-github-files": "rimraf .nojekyll 404.html"
  },
  "keywords": [],
  "author": "Maksim Kostromin <daggerok@gmail.com> (https://github.com/daggerok)",
  "license": "MIT",
  "devDependencies": {
    "gh-pages": "1.2.0",
    "ncp": "2.0.0",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "serve": "9.4.0"
  }
}
